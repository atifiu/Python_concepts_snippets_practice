   "source": [
    "dict1 = {'a': 1, 'b': 2, 'c': 3}\n",
    "dict2 = {k.upper():v*100 for (k,v) in dict1.items()}\n",
    "# Here dictionary.items returns key value pair\n",
    "print(dict2)"
   ]
